{
  "name": "jira-copy标题插件",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "这是一个快速复制标题的插件",
  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "js": ["/js/jquery-3.7.1.min.js", "/js/utils.js", "/js/content.js"],
    "run_at": "document_idle"
  }],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["storage", "activeTab", "scripting"]
}